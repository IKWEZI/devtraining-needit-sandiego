<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description> when User with VIP=true is selected as Caller in the Incident then addFieldMessage to Caller field saying "This user is VIP". Remove the message if Caller is changed then to empty or not VIP.</description>
        <field>caller_id</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Incident VIP Caller</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	
	
	var caller= g_form.getReference('caller_id', getCallerID);
	
	function getCallerID(caller){
		if (caller.vip === 'true'){
			g_form.showFieldMsg('caller_id', 'This user is VIP', 'info');
		}
		else {
			g_form.hideFieldMsg('caller_id', true);
		}
    }
   
}]]></script>
